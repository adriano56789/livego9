{
  "name": "livego-backend-real",
  "version": "1.0.0",
  "description": "Backend Real LiveGo para VPS",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "start": "nodemon --exec node --loader ts-node/esm server.ts",
    "build": "tsc",
    "serve": "node --loader ts-node/esm server.ts",
    "dev": "nodemon --exec node --loader ts-node/esm server.ts",
    "seed:categories": "node scripts/seedCategories.js",
    "seed:users": "ts-node --esm scripts/seedRealUsers.ts",
    "seed:conversations": "ts-node --esm scripts/seedConversations.ts",
    "list:users": "ts-node --esm scripts/listUsers.ts"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mongodb": "^4.0.6",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^20.19.27",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
